{
  "name": "collision-shape-editor",
  "version": "0.0.1",
  "description": "A 2D collision shape editor.",
  "scripts": {
    "old:start": "craco start",
    "old:build": "craco build",
    "old:test": "craco test",
    "old:test:coverage": "craco test --coverage --watchAll",
    "lint": "eslint src/",
    "start": "webpack serve --mode development --open --hot",
    "build": "webpack --mode production"
  },
  "keywords": [
    "physics",
    "2D",
    "collision",
    "shapes"
  ],
  "author": "Nils Stolpe",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "chroma-js": "^2.4.2",
    "color-picker": "file:../color-picker/color-picker-0.0.5.tgz",
    "deep-equal": "^2.1.0",
    "pixi-viewport": "^4.37.0",
    "pixi.js": "^6.5.8",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pixi-fiber": "1.0.4",
    "uuid": "^9.0.0"
  },
  "babel": {
    "plugins": [
      "@emotion"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-rewire": "^1.2.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "portfinder": "^1.0.32",
    "prettier": "^2.7.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-glsl-loader": "^1.0.1"
  }
}
